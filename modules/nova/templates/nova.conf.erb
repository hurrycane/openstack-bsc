--verbose
--allow_admin_api
--scheduler_driver=nova.scheduler.simple.SimpleScheduler
--dhcpbridge_flagfile=/opt/stack/nova/bin/nova.conf
--fixed_range=10.0.0.0/24
--network_manager=nova.network.manager.FlatDHCPManager
--my_ip=<%= ipaddress_eth0 %>
--public_interface=eth0
--vlan_interface=eth0
--sql_connection=mysql://root:<%= mysql_password %>@localhost/nova
--libvirt_type=qemu
--instance_name_template=instance-%08x
--osapi_extension=nova.api.openstack.v2.contrib.standard_extensions
--osapi_extension=extensions.admin.Admin
#--vncproxy_url=http://10.59.62.96:6080
#--vncproxy_wwwroot=/opt/stack/noVNC/
--api_paste_config=/opt/stack/nova/bin/nova-api-paste.ini
--image_service=nova.image.glance.GlanceImageService
--ec2_dmz_host=<%= ipaddress_eth0 %>
--rabbit_host=localhost
--glance_api_servers=1<%= ipaddress_eth0 %>:9292
--force_dhcp_release
--flat_network_bridge=br100
--flat_interface=eth0
